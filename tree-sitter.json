{
  "grammars": [
    {
      "name": "asciidoc",
      "camelcase": "AsciiDoc",
      "scope": "source.asciidoc",
      "file-types": ["adoc"],
      "injection-regex": "^asciidoc$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "AsciiDoc grammar for tree-sitter",
    "authors": [
      {
        "name": "Christoph Zirkelbach"
      }
    ],
    "links": {
      "repository": "https://github.com/tigion/tree-sitter-asciidoc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}

